import java.util.*;
class Soma{
	private String name;
	private int age;
	public void setname(String name){
		this.name=name;
	}
	public void setage(int age){
		this.age=age;
	}
	public String  getname(){
	   return name;
	}
	public int getage(){
		return age;
	}
}
class Getset{
